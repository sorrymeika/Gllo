@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Gllo企业管理平台</title>
    <link type="text/css" href="/Content/themes/manage/global.css" rel="Stylesheet" />
    <link type="text/css" href="/Content/themes/manage/guide.css" rel="Stylesheet" />
    <script type="text/javascript" src="/Scripts/abs.js"></script>
    <script type="text/javascript" src="/Scripts/grid.js"></script>
    <script type="text/javascript" src="/Scripts/manage/guide.js"></script>
</head>
<body>
    <div class="head">
        <div class="logo">
        </div>
        <div class="curUser">
            <span>当前用户：</span> <b>@(Gllo.Sys.AppData.SessionUser.UserName)</b> [<a href="/Manage/Home/Logout.html">退出登录</a>|<a href="/User/Password.html">修改密码</a>]
        </div>
    </div>
    <div class="nav">
        <div id="nav" class="navIns">
        </div>
    </div>
    <div class="main">
        <div id="sidebar" class="sidebar">
        </div>
        <div id="content">
            <iframe id="contentIframe" name="contentIframe" frameborder="0" scrolling="yes"></iframe>
        </div>
    </div>
    <script type="text/javascript">
        if(!$.ie)
            $("contentIframe").scrolling="auto";
    </script>
</body>
</html>
