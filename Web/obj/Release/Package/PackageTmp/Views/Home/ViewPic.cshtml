@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <script type="text/javascript">
        if(window.frameElement&&window.parent.handleViewPic) {
        @if (ViewBag.success)
        {
            <text>
            var data = {success: true, guid: "@ViewBag.guid",name:"@ViewBag.name",id:"@ViewBag.id"};
            </text>
        }
        else
        {
            <text>
            var data = {success: false, msg:"@ViewBag.msg"};
            </text>
        }
            window.parent.handleViewPic(data);
        }
    </script>
</body>
</html>
