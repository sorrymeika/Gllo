@{
    Layout = null;
}
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="zh_cn" lang="zh_cn">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>potoT</title>
    <script language="javascript">
        AC_FL_RunContent=0;
    </script>
    <script src="/Content/Show/AC_RunActiveContent.js" language="javascript"></script>
    <style type="text/css">
        body { margin: 0px; padding: 0px; }
    </style>
</head>
<body bgcolor="#666666">
    <!--影片中使用的 URL-->
    <!--影片中使用的文本-->
    <!-- saved from url=(0013)about:internet -->
    <script language="javascript">
        if(AC_FL_RunContent==0) {
            alert("此页需要 AC_RunActiveContent.js");
        } else {
            AC_FL_RunContent(
			'codebase','http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,0,0',
			'width','690',
			'height','517',
			'src','/Photo/@(ViewBag.id)/potoT',
			'quality','high',
			'pluginspage','http://www.macromedia.com/go/getflashplayer',
			'align','middle',
			'play','true',
			'loop','true',
			'scale','normal',
			'wmode','window',
			'devicefont','false',
			'id','potoT',
			'bgcolor','#666666',
			'name','potoT',
			'menu','true',
			'allowFullScreen','true',
			'allowScriptAccess','sameDomain',
			'movie','/Photo/@(ViewBag.id)/potoT',
			'salign',''
			); //end AC code
        }
    </script>
    <noscript>
        <object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,0,0" width="690" height="517" id="potoT" align="middle">
            <param name="allowScriptAccess" value="sameDomain" />
            <param name="allowFullScreen" value="false" />
            <param name="movie" value="/Photo/@(ViewBag.id)/potoT.swf" />
            <param name="quality" value="high" />
            <param name="bgcolor" value="#666666" />
            <embed src="/Photo/@(ViewBag.id)/potoT.swf" quality="high" bgcolor="#666666" width="690" height="517" name="potoT" align="middle" allowscriptaccess="sameDomain" allowfullscreen="false" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" />
        </object>
    </noscript>
</body>
</html>
